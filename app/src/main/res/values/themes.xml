<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TikKT" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/bunker</item>
        <item name="colorPrimaryVariant">@color/cloud_burst</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/cloud_burst</item>
        <item name="colorSecondaryVariant">@color/cloud_burst</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/cloud_burst</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.TikKT.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TikKT.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TikKT.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>